{
  "name": "amazon-oss-attribution-builder",
  "version": "0.9.0",
  "description": "A website to help you create attribution documents for product releases",
  "repository": {
    "type": "git",
    "url": "https://github.com/amzn/oss-attribution-builder"
  },
  "author": "Jacob Peddicord <peddicor@amazon.com>",
  "license": "Apache-2.0",
  "engines": {
    "node": "^8.0.0"
  },
  "scripts": {
    "build": "tsc && webpack && npm run static",
    "static": "mkdir -p build/server && cp -r config build && cp -r assets build/server && yaml2json docs/openapi.yaml > build/server/api/openapi.json",
    "dev": "export NODE_ENV=development CONFIG_NAME=dev && npm run build && concurrently -k \"docker-compose -f docker-compose.dev.yml up\" \"tsc -w\" \"nodemon -d 1 -w build build/server/localserver.js\" \"webpack-dev-server\"",
    "start": "export NODE_ENV=production && npm run build && node ./build/server/localserver.js",
    "test": "npm run build && jasmine 'build/server/**/*.spec.js'",
    "test-ui": "trap 'docker-compose -f docker-compose.selenium.yml stop' INT HUP EXIT; docker-compose -f docker-compose.selenium.yml up -d --build && tsc && jasmine 'build/spec/selenium/*.spec.js'",
    "lint": "prettier --write '**/*.{ts,tsx,js,css,scss}' '!build/**/*' && tslint -c tslint.json -p . --fix && tslint -c tslint.json -p browser --fix"
  },
  "files": [
    "build/**/*"
  ],
  "bin": {
    "oss-attribution-builder": "build/server/localserver.js"
  },
  "devDependencies": {
    "@types/body-parser": "1.16.4",
    "@types/bootstrap": "^3.3.39",
    "@types/compression": "0.0.33",
    "@types/history": "^4.6.0",
    "@types/jasmine": "^2.8.7",
    "@types/jquery": "^3.3.1",
    "@types/mockery": "^1.4.29",
    "@types/mz": "0.0.31",
    "@types/node": "^6.0.110",
    "@types/node-fetch": "^1.6.9",
    "@types/passport": "^0.3.3",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/react-redux": "^5.0.20",
    "@types/react-router-dom": "^4.2.6",
    "@types/react-select": "^1.2.6",
    "@types/selenium-webdriver": "^3.0.3",
    "@types/webpack": "^4.4.27",
    "bootstrap": "^4.3.1",
    "concurrently": "^3.5.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "history": "^4.6.3",
    "jasmine": "^2.99.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts-console-reporter": "^3.1.1",
    "jquery": "^3.4.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mockery": "^2.1.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.17.4",
    "pg-monitor": "^0.8.5",
    "popper.js": "^1.14.3",
    "prettier": "^1.14.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.2",
    "react-select": "^1.2.1",
    "redux": "^3.7.2",
    "sass-loader": "^7.1.0",
    "selenium-webdriver": "^3.0.1",
    "ts-loader": "^5.3.3",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.12.0",
    "tslint-react": "^3.6.0",
    "typescript": "~2.6.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.0",
