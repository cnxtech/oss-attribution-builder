    "yamljs": "^0.3.0"
  },
  "dependencies": {
    "compression": "^1.7.2",
    "core-js": "^2.5.6",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "immutable": "^3.7.6",
    "moment": "^2.29.2",
    "mz": "^2.4.0",
    "node-fetch": "^1.6.3",
    "passport": "^0.4.0",
    "pg-promise": "^6.3.8",
    "source-map-support": "^0.4.0",
    "spdx-license-list": "^4.1.0",
    "swagger-ui-express": "^4.0.2",
    "tiny-attribution-generator": "^0.1.1",
    "uuid": "^3.3.2",
    "whatwg-fetch": "^2.0.4",
    "winston": "^3.2.1"
  },
  "optionalDependencies": {
